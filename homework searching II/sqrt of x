
class Solution {
    public int mySqrt(int num) {
         int start = 1;
        int end = num;
        int mid;
        while(start <= end){
            mid = (start + end)/2;
            if(num/mid == mid){
                return mid;
            }
            else if(mid > num/mid){
                end = mid-1;
            }
            else{
                start = mid+1;
            }
        }
        return end;
        
    }
}
